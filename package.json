{
  "name": "ngx-localstorage",
  "version": "1.2.0",
  "description": "An Angular (>=5.0.0) wrapper for local storage access.",
  "scripts": {
    "cleanup": "rimraf bundles",
    "bundling": "rollup -c",
    "minify": "uglifyjs bundles/ngx-localstorage.umd.js --screw-ie8 --compress --mangle --comments --output bundles/ngx-localstorage.umd.min.js",
    "build": "npm run cleanup && ngc && npm run bundling && npm run minify",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bohoffi/ngx-localstorage.git"
  },
  "keywords": [
    "angular",
    "local storage",
    "wrapper",
    "directive"
  ],
  "author": "bohoffi (bo.hoffi@gmail.com)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/bohoffi/ngx-localstorage/issues"
  },
  "homepage": "https://github.com/bohoffi/ngx-localstorage#readme",
  "module": "index.js",
  "main": "bundles/ngx-localstorage.umd.js",
  "typings": "index.d.ts",
  "dependencies": {},
  "devDependencies": {
    "@angular/common": "^5.2.1",
    "@angular/compiler": "^5.2.1",
    "@angular/compiler-cli": "^5.2.1",
    "@angular/core": "^5.2.1",
    "@types/core-js": "^0.9.45",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "rxjs": "^5.5.6",
    "typescript": "^2.5.3",
    "uglify-js": "^3.3.8",
    "zone.js": "^0.8.20"
  },
  "peerDependencies": {
    "@angular/common": ">=5.0.0",
    "@angular/core": ">=5.0.0"
  }
}
